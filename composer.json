{
    "name": "juzaweb/user-credit",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "Add credit to user",
    "extra": {
        "juzaweb": {
            "name": "User Credit",
            "domain": "user_credit",
            "providers": [
                "Juzaweb\\UserCredit\\Providers\\UserCreditServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\UserCredit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\UserCredit\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1",
        "juzaweb/payment-method": "*"
    },
    "require-dev": {
         "juzaweb/modules": "^1.0",
         "juzaweb/dev-tool": "^1.0"
    }
}
